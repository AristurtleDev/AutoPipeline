	<!-- 
	###################################################################################################################
	###	Common properties that can be overwritten in the root project.
	###################################################################################################################
	-->
	<Target Name="SetDefaultProperties">
		<PropertyGroup>
			<_contentDir>$(MSBuildProjectDirectory)\Content</_contentDir>
			<_contentFile>$(_contentDir)\Content.mgcb</_contentFile>
			<_compress Condition="'$(_compress)' == ''">false</_compress>
			<_intermediateDir Condition="'$(_intermediateDir)' == ''">$(IntermediateOutputPath)</_intermediateDir>
			<_outputDir Condition="'$(_outputDir)' == ''">$(OutDir)</_outputDir>
			<_platform Condition="'$(_platform)' == ''">$(MonoGamePlatform)</_platform>
			<_profile Condition="'$(_profile)' == ''">Reach</_profile>
			<_incremental Condition="'$(_incremental)' == ''">true</_incremental>
			<_workingDir Condition="'$(_workingDir)' == ''">$(MSBuildProjectDirectory)</_workingDir>
		</PropertyGroup>
	</Target>

	<!-- 
	###################################################################################################################
	###	Builds Content.mgcb file
	###################################################################################################################
	-->
	<Target Name="BuildTextures"
			DependsOnTargets="SetDefaultProperties"
			BeforeTargets="CollectContentReferences">

		<ItemGroup>
			<_directXFiles Include="@(MonoGameDirectXAsset)" />
			<_effectFiles Include="@(MonoGameEffectAsset)" />
			<_fbxFiles Include="@(MonoGameFbxAsset)" />
			<_h264Files Include="@(MonoGameH264Asset)" />
			<_mp3Files Include="@(MonoGameMp3Asset)" />
			<_oggFiles Include="@(MonoGameOggAsset)" />
			<_openAssetFiles Include="@(MonoGameOpenAssetLibraryAsset)" />
			<_spritefontFiles Include="@(MonoGameSpriteFontAsset)" />
			<_textureFiles Include="@(MonoGameTextureAsset)" />
			<_wavFiles Include="@(MonoGameWavAsset)" />
			<_wmaFiles Include="@(MonoGameWmaAsset)" />
			<_wmvFiles Include="@(MonoGameWmvAsset)" />
			<_xmlFiles Include="@(MonoGameXmlAsset)" />
		</ItemGroup>

		<WriteLinesToFile File="$(_contentFile)" Lines="" Overwrite="true" />
		<WriteLinesToFile File="$(_contentFile)" Lines="/outputDir:$(_outputDir)"  />
		<WriteLinesToFile File="$(_contentFile)" Lines="/intermediateDir:$(_intermediateDir)"  />
		<WriteLinesToFile File="$(_contentFile)" Lines="/platform:$(_platform)" />
		<WriteLinesToFile File="$(_contentFile)" Lines="/config:" />
		<WriteLinesToFile File="$(_contentFile)" Lines="/profile:$(_profile)" />
		<WriteLinesToFile File="$(_contentFile)" Lines="/compress:$(_compress)" />

		<ItemGroup>
			<_directXAssets Condition="%(_directXFiles.Identity) != ''"	Include="%0D%0A#begin $([MSBuild]::MakeRelative($(_contentDir), %(_directXFiles.Identity)))%0D%0A/importer:%(_directXFiles.Importer)%0D%0A/processor:%(_directXFiles.Processor)%0D%0A/processorParam:ColorKeyColor=%(_directXFiles.ColorKeyColor)%0D%0A/processorParam:ColorKeyEnabled=%(_directXFiles.ColorKeyEnabled)%0D%0A/processorParam:GenerateMipmaps=%(_directXFiles.GenerateMipMaps)%0D%0A/processorParam:GenerateTangentFrames=%(_directXFiles.GenerateTangentFrames)%0D%0A/processorParam:PremultiplyTextureAlpha=%(_directXFiles.PremultiplyTextureAlpha)%0D%0A/processorParam:PremultiplyVertexColors=%(_directXFiles.PremultiplyVertexColors)%0D%0A/processorParam:ResizeTexturesToPowerOfTwo=%(_directXFiles.ResizeTexturesToPowerOfTwo)%0D%0A/processorParam:RotationX=%(_directXFiles.RotationX)%0D%0A/processorParam:RotationY=%(_directXFiles.RotationY)%0D%0A/processorParam:RotationZ=%(_directXFiles.RotationZ)%0D%0A/processorParam:Scale=%(_directXFiles.Scale)%0D%0A/processorParam:SwapWindingOrder=%(_directXFiles.SwapWindingOrder)%0D%0A/processorParam:TextureFormat=%(_directXFiles.TextureFormat)%0D%0A/build:$([MSBuild]::MakeRelative($(_contentDir), %(_directXFiles.Identity)))" />
			<_effectAssets Condition="%(_effectFiles.Identity) != ''" Include="%0D%0A#begin $([MSBuild]::MakeRelative($(_contentDir), %(_effectFiles.Identity)))%0D%0A/importer:%(_effectFiles.Importer)%0D%0A/processor:%(_effectFiles.Processor)%0D%0A/processorParam:Importer=%(_effectFiles.Importer)%0D%0A/processorParam:Processor=%(_effectFiles.Processor)%0D%0A/processorParam:DebugMode=%(_effectFiles.DebugMode)%0D%0A/processorParam:Defines=%(_effectFiles.Defines)%0D%0A/build:$([MSBuild]::MakeRelative($(_contentDir), %(_effectFiles.Identity)))" />
			<_fbxAssets	Condition="%(_fbxFiles.Identity) != ''" Include="%0D%0A#begin $([MSBuild]::MakeRelative($(_contentDir), %(_fbxFiles.Identity)))%0D%0A/importer:%(_fbxFiles.Importer)%0D%0A/processor:%(_fbxFiles.Processor)%0D%0A/processorParam:ColorKeyColor=%(_fbxFiles.ColorKeyColor)%0D%0A/processorParam:ColorKeyEnabled=%(_fbxFiles.ColorKeyEnabled)%0D%0A/processorParam:GenerateMipmaps=%(_fbxFiles.GenerateMipMaps)%0D%0A/processorParam:GenerateTangentFrames=%(_fbxFiles.GenerateTangentFrames)%0D%0A/processorParam:PremultiplyTextureAlpha=%(_fbxFiles.PremultiplyTextureAlpha)%0D%0A/processorParam:PremultiplyVertexColors=%(_fbxFiles.PremultiplyVertexColors)%0D%0A/processorParam:ResizeTexturesToPowerOfTwo=%(_fbxFiles.ResizeTexturesToPowerOfTwo)%0D%0A/processorParam:RotationX=%(_fbxFiles.RotationX)%0D%0A/processorParam:RotationY=%(_fbxFiles.RotationY)%0D%0A/processorParam:RotationZ=%(_fbxFiles.RotationZ)%0D%0A/processorParam:Scale=%(_fbxFiles.Scale)%0D%0A/processorParam:SwapWindingOrder=%(_fbxFiles.SwapWindingOrder)%0D%0A/processorParam:TextureFormat=%(_fbxFiles.TextureFormat)%0D%0A/build:$([MSBuild]::MakeRelative($(_contentDir), %(_fbxFiles.Identity)))" />
			<_h264Assets Condition="%(_h264Files.Identity) != ''" Include="%0D%0A#begin $([MSBuild]::MakeRelative($(_contentDir), %(_h264Files.Identity)))%0D%0A/importer:%(_h264Files.Importer)%0D%0A/processor:%(_h264Files.Processor)%0D%0A/processorParam:SwapWindingOrder=%(_h264Files.SwapWindingOrder)%0D%0A/build:$([MSBuild]::MakeRelative($(_contentDir), %(_h264Files.Identity)))" />
